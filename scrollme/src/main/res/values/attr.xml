<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Options">

        <attr name="sm_tint" format="color" />
        <attr name="sm_backgroundColor" format="color" />
        <attr name="sm_icon" format="reference" />
        <attr name="sm_clickable" format="boolean" />
        <attr name="sm_focusable" format="boolean" />
        <attr name="sm_size" format="enum">

            <enum name="BIG" value="0" />
            <enum name="SMALL" value="1" />

        </attr>

    </declare-styleable>

</resources>